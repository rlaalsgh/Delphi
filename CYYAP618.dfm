  object qr_cyytb620: TUniQuery
    Connection = CERI_DMF.DB
    SQL.Strings = (
      'SELECT b.cusnam,'
      '       b.addr,'
      '       a.vit_date,'
      
        '       DECODE(a.VIT_CHECK,'#39'1'#39','#39'Y'#39') VIT_CHECK, DECODE(a.VIT_SALE,' +
        #39'1'#39','#39'Y'#39') VIT_SALE, DECODE(a.VIT_AS,'#39'1'#39','#39'Y'#39') VIT_AS, DECODE(a.VIT' +
        '_CAL,'#39'1'#39','#39'Y'#39') VIT_CAL, nvl(a.VIT_CNT,'#39'1'#39') vit_cnt,'
      
        '       DECODE(a.ORD_SCALE,'#39'1'#39','#39'Y'#39') ORD_SCALE, DECODE(a.ORD_CAL,'#39 +
        '1'#39','#39'Y'#39') ORD_CAL, DECODE(a.ORD_RETAIL,'#39'1'#39','#39'Y'#39') ORD_RETAIL, DECODE' +
        '(a.ORD_RIBBON,'#39'1'#39','#39'Y'#39') ORD_RIBBON,'
      
        '       a.ORD_JPMNAM, a.ORD_REVENUE, substr(a.VIT_DESC,1,400) vit' +
        '_desc, b.TEL_NO, b.adm_man, b.ACQ_ITEM,'
      '        TO_CHAR(SUBSTR(F_901CD('#39'CM1'#39',A.USE_JPM),1) ) USE_JPM_NM,'
      
        '             TO_CHAR(SUBSTR(F_901CD('#39'CM1'#39',A.SS_JPM),1)) SS_JPM_N' +
        'M,'
      
        '             TO_CHAR(SUBSTR(F_901CD('#39'CM1'#39',A.SUG_JPM),1)) SUG_JPM' +
        '_NM,'
      
        '             TO_CHAR(SUBSTR(F_901CD('#39'CM1'#39',A.BUY_JPM),1))  BUY_JP' +
        'M_NM,'
      
        '             TO_CHAR(SUBSTR(F_904CD(A.RQ_USR),1)) RQ_USR_NM, A.Y' +
        'Y_ACP_GUB,'
      '             a.CAR_NO, a.EMPNO, a.CUS_CD, a.CUS_SEQ,a.sap_no,'
      ''
      '       substr(f_904cd(a.EMPNO),1,20) usrnm, A.prodat'
      '  FROM CYYTB620 a, CYYTB615 b, caatb904 C'
      ' WHERE a.car_no = b.car_no'
      '   AND a.cus_cd = b.cus_cd'
      '   AND a.empno = C.saw_cd(+)'
      '   AND A.CAR_NO = :CAR_NO'
      '   AND A.EMPNO = :EMPNO'
      '   AND A.CUS_CD = :CUS_CD'
      '   AND A.CUS_SEQ = :CUS_SEQ')
